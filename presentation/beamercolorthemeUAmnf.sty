\mode<presentation>

\definecolor{mnf}{RGB}{0,101,97}
\setbeamercolor*{structure}{fg=mnf}

%\setbeamercolor*{palette primary}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor*{palette primary}{use=structure,fg=white,bg=white} % bg gibt Hintergrund des Siegels
                                                                % fg gibt Schriftfarbe des Titels
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=structure.fg!75!black}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=structure.fg!50!black}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}

%\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
\setbeamercolor*{sidebar}{use=structure,bg=white} % bg gibt sidebar Hintergrund
  
%\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar primary}{use=structure,fg=black,bg=white} % fg gibt Schriftfarbe von ausgew. subsection
                                                                        % bg gibt Hintergrund von ausgew. subsection
									% nicht ausgew. subsection ist Mischfarbe
%\setbeamercolor*{palette sidebar secondary}{fg=white}
%\setbeamercolor*{palette sidebar secondary}{fg=black!50,bg=black} % section
\setbeamercolor*{palette sidebar secondary}{fg=black,bg=white} % wie primary, aber jetzt f√ºr section
%\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=red}
\setbeamercolor*{palette sidebar quaternary}{fg=structure.fg}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
